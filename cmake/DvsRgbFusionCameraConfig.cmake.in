include("${CMAKE_CURRENT_LIST_DIR}/DvsRgbFusionCameraTargets.cmake")

# Optional: set variable for include dir
set(DvsRgbFusionCamera_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@")
